{
    "daemon": {
        "log_file":     "sensu-snmp.log",
        "log_level":    "DEBUG",
        "pid_file":     "sensu-snmp.pid",
        "user":         "nobody",
        "group":        "nogroup",
        "trap_file":    "conf/traps.json"
    },
    "dispatcher": {
        "host":         "localhost",
        "port":         3030,
        "timeout":      30
    },
    "snmp": {
        "listen_address":   "0.0.0.0",
        "listen_port":      1620,
        "mib_dir":          "conf/mibs",
        "mibs":             ["CLOUDANT-REG-MIB", 
                             "CLOUDANT-PLATFORM-MIB",
                             "CLOUDANT-DBCORE-MIB"],
        "version2": {
            "enabled":      true,
            "community":    "public"
        },
        "version3": {
            "enabled":      true,
            "users": {
            }
        }
    }
}
