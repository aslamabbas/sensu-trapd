                             "CLOUDANT-PLATFORM-MIB",
                             "CLOUDANT-DBCORE-MIB"],
        "version2": {
            "enabled":      true,
            "community":    "public"
        },
        "version3": {
            "enabled":      true,
            "users": {
            }
        }
    },
    "traps": {
        "trap_file": "conf/traps.json"
    },
    "sensu": {

    }
