{
    "daemon": {
        "log_file":     "sensu-snmp.log",
        "log_level":    "INFO",
        "pid_file":     "sensu-snmp.pid",
        "user":         "nobody",
        "group":        "nogroup",
        "trap_file":    "conf/traps.json"
    },
    "dispatcher": {
        "host":         "localhost",
        "port":         3030,
        "timeout":      30
    },
    "mibs": {
        "paths": ["conf/mibs"],
        "mibs":  ["CLOUDANT-REG-MIB",
                  "CLOUDANT-PLATFORM-MIB",
                  "CLOUDANT-DBCORE-MIB"]
    },
    "snmp": {
        "transport": {
            "listen_address":   "0.0.0.0",
            "listen_port":      1620,
            "udp": {
                "enabled": true
            },
            "tcp": {
                "enabled": true
            }
        },
        "auth": {
            "version2": {
                "enabled":      true,
                "community":    "public"
            },
            "version3": {
                "enabled":      true,
                "users": {
                }
            }
        }
    }
}
