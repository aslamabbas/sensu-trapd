{
    "cloudant-generic": {
        "trap": "CLOUDANT-PLATFORM-MIB::cloudantGenericTrap",
        "arguments": {
            "CLOUDANT-PLATFORM-MIB::cloudantTrapMessage": "message"
        },
        "output": "{message}",
        "handlers": [],
        "severity": "CRITICAL" 
    },
    "cloudant-dbcore-reboot-event": {
        "trap": "CLOUDANT-DBCORE-MIB::cloudantDbcoreRebootEvent",
        "output": "DBCore on %(hostname)s was rebooted",
        "handlers": [],
        "severity": "WARNING"
    },
    "default": {
        "output": "Unknown trap received $(oid)",
        "handlers": [],
        "severity": "CRITICAL"
    }
}
