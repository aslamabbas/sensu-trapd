{
    "cloudant-generic": {
        "trap": "CLOUDANT-PLATFORM-MIB::cloudantGenericTrap",
        "tokens": {
            "message": "CLOUDANT-PLATFORM-MIB::cloudantTrapMessage"
        },
        "output": "$(message)",
        "handlers": [],
        "severity": "CRITICAL" 
    },
    "cloudant-dbcore-reboot-event": {
        "trap": "CLOUDANT-DBCORE-MIB::cloudantDbcoreRebootEvent",
        "output": "DBCore on was rebooted $(hostname)",
        "handlers": [],
        "severity": "WARNING"
    },
    "default": {
        "output": "Unknown trap received $(oid)",
        "handlers": [],
        "severity": "CRITICAL"
    }
}
